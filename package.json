    "delete": "pm2 delete all",
    "start:dev": "pm2 start ./build/src/app.js -i 5 --attach --watch | bunyan -o short",
    "dev": "nodemon -L -r tsconfig-paths/register src/app.ts | bunyan -o short",
    "local:build": "nodemon -L -r ./build/src/app.js | bunyan -o short",
    "seeds": "npx ts-node src/seeds.ts",
    "lint:check": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier:check": "prettier --check src/**/*.{ts,json}",
    "prettier:fix": "prettier --write src/**/*.{ts,json}",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
